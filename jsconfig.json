{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~js/*": [
                "src/js/*"
            ],
            "~scss/*": [
                "src/scss/*"
            ],
            "~/*": [
                "src/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@js/*": [
                "src/js/*"
            ],
            "@scss/*": [
                "src/scss/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}